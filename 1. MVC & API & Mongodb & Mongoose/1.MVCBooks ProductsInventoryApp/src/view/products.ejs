       
<% if(locals.lastVisit){ %>
    <div class="alert alert-primary" role="alert">
        Your Last Visit was on : <%= locals.lastVisit %>
    </div>
 <% } %>
        
        <h1 class="mb-5 mb-4 header">Products</h1>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody> 
                <% products.forEach(product=>{ %>
                    <tr>
                        <th><%=product.id%></th>
                        <th><%= product.name %></th>
                        <td><%= product.des %></td>
                        <td scope="col"><%= product.price %></td>
                        <td>
                            <img src="<%= product.imgUrl %>" />
                        </td>
                        <td>
                            <a href="/update-product/<%=product.id%>" class="btn btn-warning">Update</a>
                            <!-- <a href="/delete-product/<%=product.id%>" class="btn btn-danger">Delete</a> -->
                            <button class="btn btn-danger" onclick="deletePro('<%= product.id %>')">Delete</button>
                        </td>
                        
                    </tr>
                <% }) %>
                
              
            </tbody>
        </table>
 